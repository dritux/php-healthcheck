{
    "name": "dritux/php-healthcheck",
    "description": "SDK that facilitates API healthCheck monitoring",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.3",
        "php-amqplib/php-amqplib": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Dr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/BaseTestCase.php"
        ]
    }
}
